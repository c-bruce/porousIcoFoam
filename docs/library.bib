keywords = {Asymptotic solution,Darcy's law,Membrane wings,Parabolic camber,Permeable surface},
number = {5},
pages = {534--542},
publisher = {Elsevier Masson SAS},
title = {{Aerodynamics of permeable membrane wings}},
url = {http://dx.doi.org/10.1016/j.euromechflu.2011.05.003},
volume = {30},
year = {2011}
}
@article{Warneke2001,
abstract = {We have demonstrated a 138mm3 autonomous uni-directional sensing/communication mote that optically transmits a measure of the ambient light level. We have also developed a 63mm3 autonomous bi-directional communication mote that receives an optical signal, generates a pseudorandom sequence based on this signal to emulate sensor data, then optically transmits the result, although it has only been demonstrated in a bench configuration at this time. The latter system contains a micromachined corner cube reflector, a 0.078mm3 CMOS chip that consumes 75$\mu$W, and a Mn-Ti-Li cell, but we have also demonstrated operation from an ∼2mm2 solar cell. These motes allow us to demonstrate necessary concepts of Smart Dust such as optical data transmission, data processing, energy management, miniaturization, and system integration.},
