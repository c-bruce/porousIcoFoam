}
@article{Narasimhamurthy2009,
abstract = {The three-dimensional wake flow behind a flat plate placed normal to the free stream has been investigated by means of direct numerical simulations. The Reynolds number Re based on the homogeneous inflow velocity and the uniform width d of the plate was 750. Coherent vortices were alternately shed from the sides of the plate with a frequency corresponding to a Strouhal number 0.168. The wake was distinctly turbulent downstream of the plate whereas the mean recirculation bubble extended 1.96d downstream. A steady 2D mean flow and the accompanying Reynolds stresses were obtained by averaging in time and along the span of the plate. These Reynolds-averaged statistics exhibited the same qualitative features as corresponding data from cylinder wakes. {\textcopyright} 2009 Elsevier Inc. All rights reserved.},
