title = {{Numerical investigation of the performance of interface conditions for fluid flow through a partially filled porous channel}},
url = {https://doi.org/10.1016/j.tsep.2020.100628},
volume = {20},
year = {2020}
}
@article{Mittal2021,
abstract = {     Flow past a cylinder in the presence of side walls is investigated numerically for various combinations of Reynolds numbers (    {\$}50 \backslashleqslant \backslashtextit {\{}Re{\}} \backslashleqslant 100{\$}   ) and aspect ratio (    {\$}5 \backslashleqslant AR \backslashleqslant 90{\$}   ). Various attributes such as cellular shedding, dislocations, oblique angle of vortices and their structure near the end wall are studied. The complexity of the transitions in the wake, with    {\$}\backslashtextit {\{}Re{\}}{\$}   , increases with increase in    {\$}AR{\$}   . The flow is associated with a single cell wake for    {\$}AR=20{\$}   , at all the    {\$}\backslashtextit {\{}Re{\}}{\$}   considered. For    {\$}AR{\$}      {\$}=90{\$}   the flow transitions from a one-cell to two-cell wake at    {\$}\backslashtextit {\{}Re{\}} \backslashsim 50{\$}   , to a four-cell structure at    {\$}\backslashtextit {\{}Re{\}} \backslashsim 55{\$}   to a three-cell wake at    {\$}\backslashtextit {\{}Re{\}} \backslashsim 58{\$}   , and then to a two-cell wake at    {\$}\backslashtextit {\{}Re{\}} \backslashsim 61{\$}   . The vortices near the end wall diffuse for    {\$}Re\backslashleqslant 58{\$}   whereas linkages form between adjacent vortices of opposite polarity at larger    {\$}Re{\$}   . The frequency of dislocations increases with increase in    {\$}\backslashtextit {\{}Re{\}}{\$}   . Two types of dislocations have been identified: the fork type at relatively low Re and connected fork type at larger Re . Linkages between vortices in connected fork-type dislocations may lead to ring-like structures. The end conditions and nonlinear mechanisms play a significant role in the evolution of cellular shedding. A single cell across the span is the dominant mode of shedding in the linear regime. Compared with the linear analysis, nonlinearities result in smaller oblique angles, higher shedding frequency and larger streamwise speed of the convection of vortices. },
author = {Mittal, Sanjay and Pandi, Jawahar Sivabharathy Samuthira and Hore, Mainak},
doi = {10.1017/jfm.2021.82},
file = {:Users/callumbruce/OneDrive - University of Edinburgh/03{\_}Reading/cellular-vortex-shedding-from-a-cylinder-at-low-reynolds-number.pdf:pdf},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
keywords = {vortex shedding,vortex streets,wakes},
pages = {1--34},
title = {{Cellular vortex shedding from a cylinder at low Reynolds number}},
volume = {915},
