journal = {Journal of Fluid Mechanics},
month = {sep},
number = {2},
pages = {319--326},
title = {{A model for the boundary condition of a porous material. Part 1}},
url = {https://www.cambridge.org/core/product/identifier/S0022112071002088/type/journal{\_}article},
volume = {49},
year = {1971}
}
