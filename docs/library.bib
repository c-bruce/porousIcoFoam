@article{Liu2017,
abstract = {Flow around and/or through a honeycomb disk is a common problem encountered in aggregation, filtration and sedimentation processes. Mean flow, turbulent statistics and flow structures behind a solid and a honeycomb disk were experimentally studied by 2D PIV system. The results revealed that the mean flow structure behind a honeycomb disk was quite different from that of the solid disk wake. Due to the extra fluid through the small holes on the disk, the recirculation zone of honeycomb disk was much smaller both in length and in width. Meanwhile, the entrainment of the shear layer was significantly reduced and the length of the wake was increased. As the result, the honeycomb disk drastically decreased the velocity deficit, the rate of velocity recovery. The cross-stream distributions of Reynolds normal stresses exhibited symmetry while Reynolds shear stress showed asymmetry at x/d = 1, 3, 5, respectively. Using proper orthogonal decomposition (POD), it was found that, for both solid and honeycomb disk, the instantaneous wake configuration at reflectional-symmetry-breaking (RSB) mode with hairpin vortices periodically shedding could be detected by the first two POD modes. The POD mode 3 consisted of two pairs of contours, showed well plane-symmetric large-scale vortices shedding from the mixing layers region (y = ±0.5D). Due to different wake recovery rate and the interaction between the backflow and the shear layer, mode 4 structures in the both near wakes were obviously different.},
author = {Liu, Minghou and Xie, Chen and Yao, Mengyun and Yang, Jianzhi},
doi = {10.1016/j.expthermflusci.2016.10.004},
file = {:Users/callumbruce/OneDrive - University of Edinburgh/03{\_}Reading/Liu et al. - 2017 - Study on the near wake of a honeycomb disk.pdf:pdf},
issn = {08941777},
journal = {Experimental Thermal and Fluid Science},
keywords = {Bluff body wake,Flow structure,Honeycomb,Recirculation region},
pages = {33--42},
publisher = {Elsevier Inc.},
title = {{Study on the near wake of a honeycomb disk}},
url = {http://dx.doi.org/10.1016/j.expthermflusci.2016.10.004},
volume = {81},
year = {2017}
}
@article{Sahu2019,
abstract = {Flow-induced vibration (FIV) of an elastically mounted circular cylinder with an attached splitter plate in uniform flow is studied numerically via a stabilized space-time finite element method. The Reynolds number based on the cylinder diameter and the free-stream speed is restricted to 150. The ratio of the density of the body to that of the fluid, for the major part of the study, is 10. Two different reduced speeds are defined to quantify the compliance of the elastic support and flexibility of the splitter plate, respectively: Us∗ based on the natural frequency of the spring-mass system and Up∗ based on the fundamental natural frequency of the plate. Flow past a stationary cylinder (Us∗ = 0) with a flexible splitter plate of length is studied at different values of Up∗. The vibration response of the plate exhibits lock-in with various eigenmodes of the plate in different ranges of Up∗. The onsets of these lock-in regions are abrupt and hysteretic. The elastically mounted cylinder, without the splitter plate, undergoes large-amplitude vortex-induced vibration (VIV) for 4 {\textless} Us∗ {\textless} 7. These large-amplitude oscillations are a consequence of synchronization, wherein the vortex shedding frequency locks in to the cylinder oscillation frequency. A rigid splitter plate attached to the cylinder reduces significantly the peak amplitude during VIV. Increasing the length of the plate from to only marginally affects the peak amplitude. It, however, leads to a wider range of lock-in. Unlike the case of an isolated cylinder, the lock-in and desynchronization regimes are not well demarcated in the presence of the splitter plate. Further, galloping is observed beyond a critical value of Us∗; the amplitude of vibration increases with an increase in Us∗ while the vibration frequency is relatively low and remains nearly constant. Increase in plate length delays, in terms of Us∗, the onset of galloping. It is also found that the flexibility of the plate affects the maximum oscillation amplitude in the VIV regime. It also dictates the presence/absence of galloping. The system behaves similar to an isolated cylinder for a very flexible plate. The response is devoid of galloping, but relatively large amplitude of oscillation is observed during lock-in. The behaviour of the cylinder with a stiff plate is similar to that with the rigid one. The galloping instability sets in when the flexibility of the plate is less than a certain value (Up∗ {\textless} 4.7, approximately for Us∗ = 22). The VIV and galloping are separated by a range of Us∗ in which the flow is either steady, for longer plates, or exhibits very weak vortex shedding. In the VIV regime, the plate tip and cylinder vibrate in phase for low Up∗; their motion is out of phase for larger Up∗. The change in phase is also associated with change in the frequency of vibration. At low Up∗, the frequency of vibration is close to the first natural frequency of the system, while at high it Up∗ becomes closer to the second natural frequency. The vibration amplitude of the cylinder is close to maximum in the VIV regime for Us∗ = 6. Computations for various Up∗, for Us∗ = 6 and 22, are utilized to determine optimal flexibility that leads to minimal FIV. The effect of the length of the flexible splitter plate, mass ratio and damping ratio is studied. A strategy is proposed to utilize the computations from various combinations of Us∗ and Up∗ to choose the appropriate flexibility of the attached splitter plate to minimize FIV.},
author = {Sahu, Tulsi Ram and Furquan, Mohd and Mittal, Sanjay},
doi = {10.1017/jfm.2019.699},
file = {:Users/callumbruce/OneDrive - University of Edinburgh/03{\_}Reading/numerical-study-of-flow-induced-vibration-of-a-circular-cylinder-with-attached-flexible-splitter-plate-at-low-re-.pdf:pdf},
issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {flow-structure interactions,vortex shedding,vortex streets},
pages = {551--593},
title = {{Numerical study of flow-induced vibration of a circular cylinder with attached flexible splitter plate at low Re}},
volume = {880},
year = {2019}
}
