keywords = {Detached recirculating wake,Fluid-porous interface,Permeable cylinder},
number = {1},
pages = {1--12},
publisher = {Elsevier Ltd},
title = {{Steady flow around and through a permeable circular cylinder}},
url = {http://dx.doi.org/10.1016/j.compfluid.2010.09.040},
volume = {42},
year = {2011}
}
@article{Liu2012,
abstract = {We explore the stability of flapping flight in a model system that consists of a pyramid-shaped object hovering in a vertically oscillating airflow. Such a flyer not only generates sufficient aerodynamic force to keep aloft but also robustly maintains balance during free flight. Flow visualization reveals that both weight support and orientational stability result from the periodic shedding of vortices. We explain these findings with a model of the flight dynamics, predict increasing stability for higher center of mass, and verify this counterintuitive fact by comparing top- and bottom-heavy flyers. {\textcopyright} 2012 American Physical Society.},
author = {Liu, Bin and Ristroph, Leif and Weathers, Annie and Childress, Stephen and Zhang, Jun},
