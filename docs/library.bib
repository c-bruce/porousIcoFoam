issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {aerodynamics,flow-structure interactions,swimming/flying},
pages = {1043--1067},
title = {{Bistability in the rotational motion of rigid and flexible flyers}},
volume = {849},
year = {2018}
}
